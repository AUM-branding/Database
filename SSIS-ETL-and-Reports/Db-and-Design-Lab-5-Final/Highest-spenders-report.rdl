  <DataSets>
    <DataSet Name="GetShopTotalPurchases">
      <Query>
        <DataSourceName>LEGOLocal</DataSourceName>
        <CommandText>Select replace(shop_name, '"', '') as Shop, cast(replace(SubString(total_purchases, PatIndex('%[0-9.-]%', total_purchases), 10), ',','') as decimal(10,2)) as TotalPurchases
FROM shop_owners_purchase</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Shop">
          <DataField>Shop</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TotalPurchases">
          <DataField>TotalPurchases</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
